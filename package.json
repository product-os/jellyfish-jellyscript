{
  "name": "@balena/jellyfish-jellyscript",
  "version": "3.0.101",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-jellyscript.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "Jellyscript library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --typescript --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc",
    "lint": "balena-lint --typescript lib && npm run check && deplint && depcheck --ignore-bin-package --ignores=estree,json-schema,@types/jest",
    "lint-fix": "balena-lint --typescript --fix lib",
    "unit": "jest",
    "test": "catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.7",
    "@formulajs/formulajs": "^2.6.8",
    "esprima": "^4.0.1",
    "fast-json-patch": "^3.0.0-1",
    "lodash": "^4.17.21",
    "object-hash": "^2.1.1",
    "static-eval": "^2.1.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-types": "^0.5.26",
    "@balena/lint": "^5.4.1",
    "@types/jest": "^26.0.22",
    "@types/json-schema": "^7.0.7",
    "@types/object-hash": "^2.1.0",
    "@types/static-eval": "^0.2.30",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.4.0",
    "deplint": "^1.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "simple-git-hooks": "^2.3.1",
    "ts-jest": "^26.5.5",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
