{
  "name": "@balena/jellyfish-jellyscript",
  "version": "7.0.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-jellyscript.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "Jellyscript library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib && deplint",
    "lint:fix": "balena-lint --fix lib",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@formulajs/formulajs": "2.6.10",
    "esprima": "^4.0.1",
    "fast-json-patch": "^3.1.1",
    "lodash": "^4.17.21",
    "object-deep-search": "0.0.7",
    "object-hash": "^3.0.0",
    "static-eval": "^2.1.0"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.6",
    "@balena/jellyfish-types": "^2.0.5",
    "@balena/lint": "^6.2.0",
    "@types/jest": "^27.4.1",
    "@types/json-schema": "^7.0.11",
    "@types/object-hash": "^2.2.1",
    "@types/static-eval": "^0.2.31",
    "deplint": "^1.1.3",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.1.4",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-04-15T14:45:49.720Z"
  }
}
