{
  "name": "@balena/jellyfish-jellyscript",
  "version": "7.11.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-jellyscript.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "description": "Jellyscript library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ],
    "*.{json,yml}": [
      "prettier -w"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib && deplint && prettier --check **/*.json **/*.yml",
    "lint:fix": "balena-lint --fix lib && prettier -w **/*.json **/*.yml",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@formulajs/formulajs": "2.6.10",
    "esprima": "^4.0.1",
    "fast-json-patch": "^3.1.1",
    "lodash": "^4.17.21",
    "object-deep-search": "0.0.7",
    "object-hash": "^3.0.0",
    "static-eval": "^2.1.0"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.1",
    "@types/jest": "^29.2.2",
    "@types/json-schema": "^7.0.11",
    "@types/object-hash": "^3.0.0",
    "@types/static-eval": "^0.2.31",
    "autumndb": "^22.2.167",
    "deplint": "^1.1.3",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^4.0.0",
    "simple-git-hooks": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.20",
    "typescript": "^4.8.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2023-02-02T20:49:40.797Z"
  }
}
