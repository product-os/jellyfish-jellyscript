{
  "name": "@balena/jellyfish-jellyscript",
  "version": "4.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-jellyscript.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "Jellyscript library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib && npm run check && deplint && depcheck --ignore-bin-package --ignores=estree,json-schema,@types/jest",
    "lint-fix": "balena-lint --fix lib",
    "unit": "jest",
    "test": "npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.66",
    "@balena/jellyfish-client-sdk": "^5.4.1",
    "@balena/jellyfish-logger": "^3.0.54",
    "@formulajs/formulajs": "2.6.10",
    "esprima": "^4.0.1",
    "fast-json-patch": "^3.0.0-1",
    "lodash": "^4.17.21",
    "object-deep-search": "0.0.7",
    "object-hash": "^2.2.0",
    "static-eval": "^2.1.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-config": "^1.4.6",
    "@balena/jellyfish-types": "^0.8.12",
    "@balena/lint": "^6.1.1",
    "@types/jest": "^26.0.24",
    "@types/json-schema": "^7.0.8",
    "@types/object-hash": "^2.1.1",
    "@types/static-eval": "^0.2.31",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "simple-git-hooks": "^2.5.1",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-07-27T01:38:11.172Z"
  }
}
